public class word_puzzle
{
	String[][] my_table = new String[4][4];
	public word_puzzle()
	{ 
		my_table[0] = {"t", "h", "i", "s"};
				{"w", "a", "t", "s"},
				{"o", "a", "h", "g"},
				{"f", "g", "d", "t"} };
	}
	int[][] find_word(String word)
	{
		int[][] Word = new int[2][2];
		return Word;
	}
}